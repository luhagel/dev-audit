<h1>Your Teams</h1>
<% @teams.each do |team| %>
  <div class="row">
    <div class="col s12">
      <div class="card">
        <div class="card-content">
          <span class="card-title"><%= link_to team.name, team %></span>
        </div>
      </div>
    </div>
  </div>
<% end %>
<br>
<%= link_to "New Team", new_team_path, class: "btn waves-effect wave-light" %>
