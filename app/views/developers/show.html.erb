<div class="card is-fullwidth">
  <header class="card-header">
    <p class="card-header-title">
      Overview: <%= @developer.github_user.login %>
    </p>
  </header>
  <div class="card-content">
    <%= render 'github_users/contrib_graph', contribs: @developer.github_user.contributions %>
  </div>
  <footer class="card-footer">
    <%= link_to "Back", team_path(@team), class: "card-footer-item" %>
    <a href="https://github.com/<%= @developer.username %>" target=_blank class="card-footer-item">Github Profile</a>
  </footer>
</div>
