<h1 class="text-xs-center">Overview: <%= @developer.name %></h1>
<div class="row card">
  <div class="card-content">
    <div class="col s4">
      Github Profile:
    </div>
    <div class="col s8">
      <a href="https://github.com/<%= @developer.username %>" target=_blank><%= @developer.username %></a>
    </div>
  </div>
  <div class="row">
    <div class="col s4">
      Contribution Graph:
    </div>
    <div class="col s8">
      <%== @developer.git_graph_html %>
    </div>
  </div>
</div>
<%= link_to "Back", team_path(@team), class: "btn waves-effect waves-light" %>